#include <stdio.h>
int main() {
    int num, N;
    int min, max;
//Chiedo all'utente quanti numeri vuole inserire
    printf("Inserisci il numero di elementi: ");
    scanf("%d", &N);
    
    for (int i = 1; i <= N; i++) {
        printf("Inserisci il %dÂ° numero: ", i);
        scanf("%d", &num);

        if (num < min) {
            min = num;
        }

        if (num > max) {
            max = num;
        }
    }

    printf("Il massimo e' %d\n", max);
    printf("Il minimo e' %d\n", min);
    
}
